diff -uNr linux-2.6.32/usr/include/linux/virtio_ids.h clean_modified/linux-2.6.32/usr/include/linux/virtio_ids.h
--- linux-2.6.32/usr/include/linux/virtio_ids.h	1969-12-31 18:00:00.000000000 -0600
